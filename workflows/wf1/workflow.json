{
    "workflow": {
        "steps": [
            {
                "id": "fetch-metadata",
                "adaptor": "http",
                "expression": "0-fetch-metadata.js",
                "state": {
                    "manualCursor": "2024-06-04"
                },
                "next": {
                    "get-teis": "!state.errors"
                }
            },
            {
                "id": "get-teis",
                "adaptor": "dhis2",
                "configuration": "../tmp/dhis2-creds.json",
                "expression": "1-get-teis.js",
                "next": {
                    "get-locations": "!state.errors"
                }
            },
            {
                "id": "get-locations",
                "adaptor": "dhis2",
                "configuration": "../tmp/dhis2-creds.json",
                "expression": "2-get-locations.js",
                "next": {
                    "create-patients": "!state.errors"
                }
            },
            {
                "id": "create-patients",
                "adaptor": "openmrs",
                "configuration": "../tmp/openmrs-creds.json",
                "expression": "3-create-patients.js",
                "next": {
                    "update-teis": true
                }
            },
            {
                "id": "update-teis",
                "adaptor": "openmrs",
                "configuration": "../tmp/openmrs-creds.json",
                "expression": "4-update-teis.js"
            }
        ]
    }
}